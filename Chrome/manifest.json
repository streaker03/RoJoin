{
  "manifest_version": 2,
  "name": "RoJoin",
  "version": "1.0.2",

  "description": "Finds a server with a certain amount of players.",

  "icons": {
    "48": "icon48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.roblox.com/games/*"],
      "js": ["rojoin.js"]
    }
  ],

  "permissions": [
    "*://*.roblox.com/games/*"
  ]
}